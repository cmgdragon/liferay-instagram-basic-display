{
	"name": "liferay-instagram-basic-display",
	"version": "1.0.0",
	"description": "Liferay Instagram Basic Display",
	"devDependencies": {
		"liferay-npm-bundler": "^2.18.7",
		"liferay-npm-build-support": "^2.18.7",
		"copy-webpack-plugin": "4.6.0",
		"webpack": "4.29.6",
		"webpack-cli": "3.3.0",
		"webpack-dev-server": "3.2.1",
		"babel-cli": "6.26.0",
		"babel-preset-env": "1.7.0",
		"babel-preset-react": "6.24.1",
		"babel-loader": "7.1.5"
	},
	"dependencies": {
		"babel-polyfill": "^6.26.0",
		"react": "16.8.6",
		"react-append-head": "^1.0.11",
		"react-dom": "16.8.6",
		"react-image-lightbox": "^5.1.1",
		"react-slick": "^0.26.1",
		"slick-carousel": "^1.8.1"
	},
	"scripts": {
		"build": "babel --source-maps -d build src && npm run copy-assets && liferay-npm-bundler",
		"copy-assets": "lnbs-copy-assets",
		"translate": "lnbs-translate",
		"deploy": "npm run build && lnbs-deploy",
		"start": "lnbs-start"
	},
	"portlet": {
		"com.liferay.portlet.display-category": "category.sample",
		"com.liferay.portlet.header-portlet-css": "/css/styles.css",
		"com.liferay.portlet.instanceable": true,
		"javax.portlet.name": "liferayinstagrambasicdisplay",
		"javax.portlet.security-role-ref": "power-user,user",
		"javax.portlet.resource-bundle": "content.Language"
	},
	"main": "index.js"
}
